{
  "name": "array-matchers",
  "version": "0.1.2",
  "description": "For precise array matching only",
  "author": "Yoeri Nijs",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/YoeriNijs/array-matchers.git"
  },
  "license": "GPL-3.0",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "test:watch": "jest --watchAll --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint && npm run test"
    }
  },
  "files": [
    "lib/**/*"
  ],
  "keywords": [
    "array",
    "flatten",
    "utility",
    "collections",
    "split",
    "predicate"
  ],
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "add-matchers": "^0.6.2",
    "husky": "^4.3.0",
    "jest": "^26.5.0",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "pincet": "^0.4.0"
  },
  "bugs": {
    "url": "https://github.com/YoeriNijs/array-matchers/issues"
  },
  "homepage": "https://github.com/YoeriNijs/array-matchers#readme",
  "directories": {
    "lib": "lib"
  }
}
